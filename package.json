{
  "name": "Airbnb-booking",
  "version": "1.0.0",
  "description": "> Airbnb Booking module",
  "main": "server/build/server.bundle.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build exec \"node server/build/server.bundle.js\"",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "prod:build": "wepack --config webpack.config.prod.js",
    "seed": "mysql -u root <db/schema.sql && node db/room_generater.js && db/booking_generater.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --bail --findRelatedTests"
    ],
    "*.css": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
